<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="account_financial_report_l10n_vn_pl" model="account.report">
        <field name="name">Profit and Loss</field>
        <field name="root_report_id" ref="account_reports.profit_and_loss"/>
        <field name="filter_analytic_groupby" eval="True"/>
        <field name="filter_unfold_all" eval="True"/>
        <field name="filter_journals" eval="True"/>
        <field name="filter_multi_company">selector</field>
        <field name="filter_budgets" eval="True"/>
        <field name="country_id" ref="base.vn"/>
        <field name="default_opening_date_filter">this_year</field>
        <field name="column_ids">
            <record id="account_financial_report_l10n_vn_pl_code" model="account.report.column">
                <field name="name">Code</field>
                <field name="expression_label">code</field>
                <field name="figure_type">string</field>
            </record>
            <record id="account_financial_report_l10n_vn_pl_balance" model="account.report.column">
                <field name="name">Balance</field>
                <field name="expression_label">balance</field>
            </record>
        </field>
        <field name="line_ids">
            <record id="account_financial_report_l10n_vn_pl_rssp" model="account.report.line">
                <field name="name">1. Revenues from sales and services rendered</field>
                <field name="code">RSSP</field>
                <field name="hierarchy_level" eval="1"/>
                <field name="expression_ids">
                    <record id="account_financial_report_l10n_vn_pl_rssp_code" model="account.report.expression">
                        <field name="label">code</field>
                        <field name="auditable" eval="False"/>
                        <field name="engine">aggregation</field>
                        <field name="formula">1</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_rssp_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">account_codes</field>
                        <field name="formula">-511</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_l10n_vn_pl_rd" model="account.report.line">
                <field name="name">2. Revenue deductions</field>
                <field name="code">RD</field>
                <field name="hierarchy_level" eval="1"/>
                <field name="expression_ids">
                    <record id="account_financial_report_l10n_vn_pl_rd_code" model="account.report.expression">
                        <field name="label">code</field>
                        <field name="auditable" eval="False"/>
                        <field name="engine">aggregation</field>
                        <field name="formula">2</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_rd_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">account_codes</field>
                        <field name="formula">521</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_l10n_vn_pl_nrssr" model="account.report.line">
                <field name="name">3. Net revenues from sales and services rendered (10=01-02)</field>
                <field name="code">NRSSR</field>
                <field name="hierarchy_level" eval="1"/>
                <field name="expression_ids">
                    <record id="account_financial_report_l10n_vn_pl_nrssr_code" model="account.report.expression">
                        <field name="label">code</field>
                        <field name="auditable" eval="False"/>
                        <field name="engine">aggregation</field>
                        <field name="formula">10</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_nrssr_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">RSSP.balance - RD.balance</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_l10n_vn_pl_cgs" model="account.report.line">
                <field name="name">4. Costs of goods sold</field>
                <field name="code">CGS</field>
                <field name="hierarchy_level" eval="1"/>
                <field name="expression_ids">
                    <record id="account_financial_report_l10n_vn_pl_cgs_code" model="account.report.expression">
                        <field name="label">code</field>
                        <field name="auditable" eval="False"/>
                        <field name="engine">aggregation</field>
                        <field name="formula">11</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_cgs_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">domain</field>
                        <field name="formula" eval="[('account_id.code', '=like', '632%')]"/>
                        <field name="subformula">sum</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_l10n_vn_pl_grssr" model="account.report.line">
                <field name="name">5. Gross profit from sales and services rendered (20=10-11)</field>
                <field name="code">GRSSR</field>
                <field name="hierarchy_level" eval="1"/>
                <field name="expression_ids">
                    <record id="account_financial_report_l10n_vn_pl_grssr_code" model="account.report.expression">
                        <field name="label">code</field>
                        <field name="auditable" eval="False"/>
                        <field name="engine">aggregation</field>
                        <field name="formula">20</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_grssr_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">NRSSR.balance - CGS.balance</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_l10n_vn_pl_fi" model="account.report.line">
                <field name="name">6. Financial income</field>
                <field name="code">FI</field>
                <field name="hierarchy_level" eval="1"/>
                <field name="expression_ids">
                    <record id="account_financial_report_l10n_vn_pl_fi_code" model="account.report.expression">
                        <field name="label">code</field>
                        <field name="auditable" eval="False"/>
                        <field name="engine">aggregation</field>
                        <field name="formula">21</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_fi_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">account_codes</field>
                        <field name="formula">-515</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_l10n_vn_pl_fe" model="account.report.line">
                <field name="name">7. Financial expenses</field>
                <field name="code">FE</field>
                <field name="hierarchy_level" eval="1"/>
                <field name="expression_ids">
                    <record id="account_financial_report_l10n_vn_pl_fe_code" model="account.report.expression">
                        <field name="label">code</field>
                        <field name="auditable" eval="False"/>
                        <field name="engine">aggregation</field>
                        <field name="formula">22</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_fe_account_codes" model="account.report.expression">
                        <field name="label">account_codes</field>
                        <field name="engine">account_codes</field>
                        <field name="formula">635\(6351)</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_fe_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">FE.account_codes + IE.balance</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_l10n_vn_pl_ie" model="account.report.line">
                <field name="name">- In which: Interest expenses</field>
                <field name="code">IE</field>
                <field name="hierarchy_level" eval="5"/>
                <field name="expression_ids">
                    <record id="account_financial_report_l10n_vn_pl_ie_code" model="account.report.expression">
                        <field name="label">code</field>
                        <field name="auditable" eval="False"/>
                        <field name="engine">aggregation</field>
                        <field name="formula">23</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_ie_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">account_codes</field>
                        <field name="formula">6351</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_l10n_vn_pl_se" model="account.report.line">
                <field name="name">8. Selling expenses</field>
                <field name="code">SE</field>
                <field name="hierarchy_level" eval="1"/>
                <field name="expression_ids">
                    <record id="account_financial_report_l10n_vn_pl_se_code" model="account.report.expression">
                        <field name="label">code</field>
                        <field name="auditable" eval="False"/>
                        <field name="engine">aggregation</field>
                        <field name="formula">25</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_se_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">account_codes</field>
                        <field name="formula">641</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_l10n_vn_pl_gae" model="account.report.line">
                <field name="name">9. General administration expenses</field>
                <field name="code">GAE</field>
                <field name="hierarchy_level" eval="1"/>
                <field name="expression_ids">
                    <record id="account_financial_report_l10n_vn_pl_gae_code" model="account.report.expression">
                        <field name="label">code</field>
                        <field name="auditable" eval="False"/>
                        <field name="engine">aggregation</field>
                        <field name="formula">26</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_gae_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">account_codes</field>
                        <field name="formula">642</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_l10n_vn_pl_npoa" model="account.report.line">
                <field name="name">10. Net profits from operating activities {30 = 20 + (21 - 22) - (25 + 26)}</field>
                <field name="code">NPOA</field>
                <field name="hierarchy_level" eval="0"/>
                <field name="expression_ids">
                    <record id="account_financial_report_l10n_vn_pl_npoa_code" model="account.report.expression">
                        <field name="label">code</field>
                        <field name="auditable" eval="False"/>
                        <field name="engine">aggregation</field>
                        <field name="formula">30</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_npoa_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">GRSSR.balance + (FI.balance - FE.balance) - (SE.balance + GAE.balance)</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_l10n_vn_pl_oi" model="account.report.line">
                <field name="name">11. Other income</field>
                <field name="code">OI</field>
                <field name="hierarchy_level" eval="1"/>
                <field name="expression_ids">
                    <record id="account_financial_report_l10n_vn_pl_oi_code" model="account.report.expression">
                        <field name="label">code</field>
                        <field name="auditable" eval="False"/>
                        <field name="engine">aggregation</field>
                        <field name="formula">31</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_oi_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">account_codes</field>
                        <field name="formula">-711</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_l10n_vn_pl_oe" model="account.report.line">
                <field name="name">12. Other expenses</field>
                <field name="code">OE</field>
                <field name="hierarchy_level" eval="1"/>
                <field name="expression_ids">
                    <record id="account_financial_report_l10n_vn_pl_oe_code" model="account.report.expression">
                        <field name="label">code</field>
                        <field name="auditable" eval="False"/>
                        <field name="engine">aggregation</field>
                        <field name="formula">32</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_oe_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">account_codes</field>
                        <field name="formula">811</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_l10n_vn_pl_op" model="account.report.line">
                <field name="name">13. Other profits (40 = 31 - 32)</field>
                <field name="code">OP</field>
                <field name="hierarchy_level" eval="1"/>
                <field name="expression_ids">
                    <record id="account_financial_report_l10n_vn_pl_op_code" model="account.report.expression">
                        <field name="label">code</field>
                        <field name="auditable" eval="False"/>
                        <field name="engine">aggregation</field>
                        <field name="formula">40</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_op_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">OI.balance - OE.balance</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_l10n_vn_pl_tnpbt" model="account.report.line">
                <field name="name">14. Total net profit before tax (50 = 30 + 40)</field>
                <field name="code">TNPBT</field>
                <field name="hierarchy_level" eval="0"/>
                <field name="expression_ids">
                    <record id="account_financial_report_l10n_vn_pl_tnpbt_code" model="account.report.expression">
                        <field name="label">code</field>
                        <field name="auditable" eval="False"/>
                        <field name="engine">aggregation</field>
                        <field name="formula">50</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_tnpbt_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">NPOA.balance + OP.balance</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_l10n_vn_pl_ccoit" model="account.report.line">
                <field name="name">15. Current corporate income tax expenses</field>
                <field name="code">CCOIT</field>
                <field name="hierarchy_level" eval="1"/>
                <field name="expression_ids">
                    <record id="account_financial_report_l10n_vn_pl_ccoit_code" model="account.report.expression">
                        <field name="label">code</field>
                        <field name="auditable" eval="False"/>
                        <field name="engine">aggregation</field>
                        <field name="formula">51</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_ccoit_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">account_codes</field>
                        <field name="formula">8211</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_l10n_vn_pl_dcit" model="account.report.line">
                <field name="name">16. Deferred corporate income tax expenses</field>
                <field name="code">DCIT</field>
                <field name="hierarchy_level" eval="1"/>
                <field name="expression_ids">
                    <record id="account_financial_report_l10n_vn_pl_dcit_code" model="account.report.expression">
                        <field name="label">code</field>
                        <field name="auditable" eval="False"/>
                        <field name="engine">aggregation</field>
                        <field name="formula">52</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_dcit_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">account_codes</field>
                        <field name="formula">8212</field>
                    </record>
                </field>
            </record>
            <record id="account_financial_report_l10n_vn_pl_paeic" model="account.report.line">
                <field name="name">17. Profits after enterprise income tax (60 = 50 – 51 – 52)</field>
                <field name="code">PAEIC</field>
                <field name="hierarchy_level" eval="0"/>
                <field name="expression_ids">
                    <record id="account_financial_report_l10n_vn_pl_paeic_code" model="account.report.expression">
                        <field name="label">code</field>
                        <field name="auditable" eval="False"/>
                        <field name="engine">aggregation</field>
                        <field name="formula">60</field>
                    </record>
                    <record id="account_financial_report_l10n_vn_pl_paeic_balance" model="account.report.expression">
                        <field name="label">balance</field>
                        <field name="engine">aggregation</field>
                        <field name="formula">TNPBT.balance - CCOIT.balance - DCIT.balance</field>
                    </record>
                </field>
            </record>
        </field>
    </record>
</odoo>
